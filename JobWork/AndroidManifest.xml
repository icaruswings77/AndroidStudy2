<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lee2day.jobwork"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".JobMainActivity"
            android:label="@string/title_activity_job_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.lee2day.jobwork.JobVisionActivity"
            android:label="@string/title_activity_job_info" >
        </activity>

        <activity
            android:name=".AptitudeTestActivity"
            android:label="@string/title_activity_aptitude" >
        </activity>
		<activity
            android:name=".AptitudeRsltActivity"
            android:label="@string/title_activity_aptitude" >
        </activity>        
        <activity
            android:name=".InterviewActivity"
            android:label="@string/title_activity_interview" >
        </activity>
        <activity
            android:name=".AdviceActivity"
            android:label="@string/title_activity_advice" >
        </activity>        
    </application>
    
 	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
 	<uses-permission android:name="android.permission.READ_OWNER_DATA"></uses-permission>    

</manifest>